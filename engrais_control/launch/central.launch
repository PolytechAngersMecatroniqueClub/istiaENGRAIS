<?xml version="1.6" ?>
<launch>

	<arg name="subscribe_topic_front" default="/engrais/laser_front/lines"/>
	<arg name="subscribe_topic_back" default="/engrais/laser_back/lines"/>
	<arg name="publish_topic_left" default="/engrais/leftWheel_controller/command"/>
	<arg name="publish_topic_right" default="/engrais/rightWheel_controller/command"/>

	<node name="engrais_central" pkg="engrais_control" type="robot_control">
		<param name="subscribe_topic_front" type="string" value="$(arg subscribe_topic_front)"/>
		<param name="subscribe_topic_back" type="string" value="$(arg subscribe_topic_back)"/>
		<param name="publish_topic_left" type="string" value="$(arg publish_topic_left)"/>
		<param name="publish_topic_right" type="string" value="$(arg publish_topic_right)"/>
	</node>

</launch>