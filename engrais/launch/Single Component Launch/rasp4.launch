<?xml version="1.6" ?>
<launch>

	<!--> Sick Tim Arguments <-->
    <arg name="rasp3_IP" default="192.168.10.104" />

	<arg name="lidar_IP" default="192.168.10.112"/>
	<arg name="sick_node_name" default="sick_2_rpi_4"/>
	<arg name="scan_topic_name" default="scan2"/>
	<arg name="rviz_frame_name" default="laser2"/>

	<!--> Find Lines Arguments <-->
	<arg name="lines_topic_name" default="lines2"/>
	<arg name="findlines_node_name" default="engrais_findlines_2"/>
    <arg name="emer_topic_name" default="none"/>


 	<machine name="rasp3" address="$(arg rasp3_IP)" user="engrais" password="engrais" env-loader="~/catkin_ws/src/istiaENGRAIS/config/config.sh" default="true"/>

	<include file="$(find engrais_control)/launch/findlines.launch">
	    <arg name="IP_Lidar" default="$(arg lidar_IP)"/>
	    <arg name="sick_node" default="$(arg sick_node_name)"/>
	    <arg name="scan_topic" default="$(arg scan_topic_name)"/>
	    <arg name="rviz_frame" default="$(arg rviz_frame_name)"/>

	    <arg name="lines_topic" default="$(arg lines_topic_name)"/>
	    <arg name="findlines_node" default="$(arg findlines_node_name)"/>
        <arg name="emer_topic" default="$(arg emer_topic_name)"/>
	</include>

</launch>
